{
  $schema: "https://docs.renovatebot.com/renovate-schema.json",
  customManagers: [
    {
      customType: "regex",
      description: "Process OCI dependencies",
      managerFilePatterns: [
        "/^kubernetes/.+\\.yaml(?:\\.j2)?$/"
      ],
      matchStrings: [
        "oci://(?<depName>[^:]+):(?<currentValue>\\S+)",
      ],
      datasourceTemplate: "docker",
    }
  ]
}
